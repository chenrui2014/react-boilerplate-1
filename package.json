{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "A boilerplate for getting up and running with React",
  "author": "Jon Gunnison",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/jgunnison/react-boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/jgunnison/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/jgunnison/react-boilerplate/issues"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "jest": "./node_modules/jest-cli/bin/jest.js --watch",
    "test": "./node_modules/jest-cli/bin/jest.js --coverage",
    "stats": "webpack -p --env.production --config config/webpack --json > stats.json",
    "prebuild": "npm run test",
    "build": "webpack -p --env.production --config config/webpack",
    "start": "webpack-dev-server --config config/webpack",
    "server": "set NODE_ENV=production && node server.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "offline-plugin": "^4.8.3",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "why-did-you-update": "^0.0.8"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili": "^0.1.4",
    "babili-webpack-plugin": "^0.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.50.0",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^1.5.0-beta.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint-no-browser-hacks": "^1.0.2",
    "stylelint-no-unsupported-browser-features": "^1.0.0",
    "stylelint-scss": "^1.5.1",
    "stylelint-webpack-plugin": "^0.8.0",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "peerDependencies": {
    "babel-cli": "^6.24.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "client-src/**/*.{js}",
      "!app/**/*.test.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules",
      "client-src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/internals/mocks/fileMock.js"
    },
    "testRegex": ".*\\.test\\.js$"
  }
}
