{
  "plugins": [
    "lodash",
    "transform-react-jsx",
    "transform-class-properties",
    "transform-flow-strip-types"
  ],
	"presets": [
		[
			"es2015", {
				"loose": true,
				"modules": false
			}
		],
		[
			"env", {
	      "loose": true,
	      "modules": false,
	      "targets": {
	        "browsers": ["last 2 versions", "not ie 10"]
	      },
        "production": {
          "plugins": [
            "transform-react-inline-elements",
            "transform-react-constant-elements",
            "transform-react-remove-prop-types"
          ]
        }
	    }
    ],
    "airbnb",
		"react",
		"stage-2"
	]
}
